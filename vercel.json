{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/cf-stream/(.*)",
      "destination": "https://customer-zl11k93xxb6833cs.cloudflarestream.com/$1"
    },
    {
      "source": "/embed",
      "destination": "/embed.html"
    },
    {
      "source": "/(.*)",
      "destination": "/embed.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}